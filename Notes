    names=n[0]
    dgs = n[1:]
    dgs=[int(i) for i in dgs]
    d.update({names[0:]:dgs[0:] for i in range(len(dgs))})
d1=sorted(d.items())
d2=sorted(d1,key=lambda v:v[1],reverse=True)
