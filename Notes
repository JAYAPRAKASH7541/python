def PRINTING(A,B):
    #print(A,B)
    for x,i in enumerate(B[:-1]):
        if i==0:
            continue
        if i >=0 :
            if A[x] == 1:
                print(" + {}x".format(i),end="")
                continue
            if x==0:
                print(" {}x^{}".format(i,A[x]),end="")
            else:    
                print(" + {}x^{}".format(i,A[x]),end="")
            
        elif i<0:
            if A[x] == 1:
                print(" - {}x".format(abs(i)),end="")
                continue
            if x==0: 
              print(" -{}x^{}".format(abs(i),A[x]),end="")
            else:
                print(" - {}x^{}".format(abs(i),A[x]),end="")
    if len(A) == 1:
        if B[-1] >=0 :
           print(" {}".format(B[-1]))
        else:
            print(" -{}".format(B[-1]))
        return
    if B[-1]>0:   
      print(" + {}".format(abs(B[-1]))) 
    else:
        print(" - {}".format(abs(B[-1]))) 

